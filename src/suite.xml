<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Testing suite" verbose="2" parallel="tests" thread-count="1"
  allow-return-values="true">

  <test name="Login tests">
    <classes>
      <class name="ai.skillo.tests.LoginTests"/>
    </classes>
  </test>

  <test name="Registration tests">
    <classes>
      <class name="ai.skillo.RegistrationTest"/>
    </classes>
  </test>

  <test name="Product test">
    <classes>
      <class name="ai.skillo.tests.ProductTest"/>
    </classes>
  </test>

</suite>